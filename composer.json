{
    "name": "aplus/aplus",
    "description": "Aplus Command Line Tool",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "aplus",
        "cli",
        "framework",
        "installer",
        "preload",
        "tool"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/aplus",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/aplus/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://gitlab.com/aplus-framework/aplus",
        "docs": "https://docs.aplus-framework.com/guides/aplus/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-posix": "*",
        "aplus/app": "dev-master",
        "aplus/framework": "dev-master",
        "aplus/one": "dev-master"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.12",
        "ergebnis/composer-normalize": "^2.25",
        "phpmd/phpmd": "^2.12",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Aplus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/aplus"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
